#define FILE_RCOS_C

#include "rcos.h"

#define DEBUG_FILE_NAME "rcos"

// #define DEBUG_PORT_
// DEBUG_START(0)

CORE_EVENTQUEUE_SIZE(%EVENTQUEUE_SIZE%)
%DEBUG_DEV_LINE%
CORE_TICK_DEFAULT(%TICK_ARGS%)
CORE_WDT_DEFAULT(%WDT_ARGS%)

%OBJECTS_CREATE%
void rcosMainLoop(void)
{
    platformInit();
    coreInit();

%OBJECTS_INIT%
    coreRun();
}
